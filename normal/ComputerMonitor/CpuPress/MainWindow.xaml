<Window x:Class="CpuPress.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CpuPress"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel DataContext="{Binding RelativeSource={RelativeSource FindAncestor,AncestorType=Window}}">
        <Button Content="计算" Click="ButtonBase_OnClick" IsEnabled="{Binding IsButtonEnable}"></Button>
        <TextBlock Text="{Binding LogValue}"></TextBlock>
    </StackPanel>
</Window>
